{
  "name": "ada-futures-bot",
  "version": "1.2.12",
  "description": "AI-Enhanced Binance Futures Trading Bot with 4-Position Hedge System and DeepSeek Integration",
  "main": "dist/multi-pair-index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/multi-pair-index.js",
    "dev": "ts-node src/multi-pair-index.ts",
    "ui": "ts-node src/ui-server.ts",
    "ui:prod": "node dist/ui-server.js",
    "ui:build": "pnpm run build && pnpm run ui:prod",
    "ui:dev": "concurrently \"ts-node src/server.ts\" \"ts-node src/multi-pair-index.ts\"",
    "test": "jest",
    "test:config": "ts-node src/scripts/test-configuration.ts",
    "show-levels": "ts-node src/scripts/show-levels.ts",
    "parse-levels": "ts-node src/scripts/parse-levels.ts",
    "comprehensive-levels": "ts-node src/scripts/comprehensive-levels.ts",
    "show-comprehensive": "ts-node src/scripts/show-comprehensive-levels.ts",
    "test-profit-taking": "ts-node src/scripts/test-profit-taking.ts",
    "analyze-price": "ts-node src/scripts/analyze-current-price.ts",
    "version": "ts-node src/scripts/show-version.ts",
    "manage-pairs": "ts-node src/scripts/manage-pairs.ts",
    "show-sizing": "ts-node src/scripts/show-sizing-recommendations.ts",
    "test-cross-pair": "ts-node src/scripts/test-cross-pair-limiting.ts",
    "test-ai": "ts-node src/scripts/test-ai-integration.ts",
    "test-hf-indicators": "ts-node src/scripts/test-high-frequency-indicators.ts",
    "test-hf-strategy": "ts-node src/scripts/test-high-frequency-strategy.ts",
    "test-hf-only": "ts-node src/scripts/test-hf-only-mode.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "trading",
    "bot",
    "crypto",
    "futures",
    "ada",
    "binance",
    "ai",
    "artificial-intelligence",
    "deepseek",
    "machine-learning",
    "sentiment-analysis",
    "pattern-recognition"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "binance-api-node": "^0.12.4",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.8.1",
    "technicalindicators": "^3.1.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
